{
  "Env": {},
  "DependsOn": [
    "fleet-protocol-interface",
    "aeron",
    "async-function-execution"
  ],
  "Git": {
    "URI": "https://github.com/bringauto/fleet-protocol-cpp.git",
    "Revision": "v1.2.0"
  },
  "Build": {
    "CMake": {
      "Defines": {
        "CMAKE_BUILD_TYPE": "Debug",
        "BRINGAUTO_INSTALL": "ON",
        "BRINGAUTO_PACKAGE": "ON",
        "BRINGAUTO_SYSTEM_DEP": "ON",
        "BRINGAUTO_TESTS": "OFF"
      }
    }
  },
  "Package": {
    "Name": "fleet-protocol-cpp",
    "VersionTag": "v1.2.0",
    "PlatformString": {
      "Mode": "auto"
    },
    "IsLibrary": true,
    "IsDevLib": true,
    "IsDebug": true
  },
  "DockerMatrix": {
    "ImageNames": [
      "ubuntu2204",
      "debian12",
      "ubuntu2404",
      "fedora40",
      "fleet-os-3",
      "fedora41",
      "fedora42"
    ]
  }
}
